{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "out",
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true
  },
  "include": [
    "**/*.ts"
],
"exclude": [
    "../node_modules",
    "../*.js"
]
}
